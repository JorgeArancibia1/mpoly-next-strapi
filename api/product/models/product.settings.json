{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Producto",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "talla": {
      "type": "string",
      "required": true
    },
    "color": {
      "type": "string"
    },
    "marca": {
      "type": "string",
      "required": true
    },
    "precio": {
      "type": "integer",
      "required": true
    },
    "imagen": {
      "type": "string",
      "required": false
    },
    "estado": {
      "type": "string",
      "required": true
    },
    "categoria": {
      "model": "categoria"
    },
    "tipo": {
      "model": "tipo"
    },
    "descuento": {
      "type": "integer"
    },
    "img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "descripcion": {
      "type": "text"
    },
    "screenshots": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "fechaSubida": {
      "type": "datetime"
    },
    "fechaVenta": {
      "type": "datetime"
    }
  }
}
